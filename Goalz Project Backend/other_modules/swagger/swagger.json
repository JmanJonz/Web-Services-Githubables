{
  "openapi": "3.0.0",
  "info": {
    "title": "Goalz App API",
    "description": "Access To Goalz App DB",
    "version": "1.0.0"
  },
  "servers": [
    {
      "url": "https://goalz-project.onrender.com/"
    },
    {
      "url": "http://goalz-project.onrender.com/"
    }
  ],
  "paths": {},
  "components": {
    "schemas": {
      "goalSchema": {
        "type": "object",
        "properties": {
          "goalTitle": {
            "type": "string",
            "example": "Stop Picking Nose"
          },
          "startDate": {
            "type": "string",
            "example": "2023/9/30"
          },
          "completeByDate": {
            "type": "string",
            "example": "2024/7/30"
          },
          "belongsToValue": {
            "type": "string",
            "example": "family"
          },
          "urgentImportantRating": {
            "type": "number",
            "example": 7
          },
          "goalCompleted": {
            "type": "boolean",
            "example": false
          },
          "userEmail": {
            "type": "string",
            "example": "bogus@dumbmail.com"
          }
        }
      },
      "userSchema": {
        "type": "object",
        "properties": {
          "email": {
            "type": "string",
            "example": "bogus@dumbmail.com"
          },
          "password": {
            "type": "string",
            "example": "1234567"
          },
          "firstName": {
            "type": "string",
            "example": "Howard"
          },
          "lastName": {
            "type": "string",
            "example": "Chowski"
          }
        }
      }
    }
  }
}